css":"* {\n  box-sizing: border-box;\n}\n\nbody {\n  grid-template-rows: 80px auto 60px;\n  font-family: sans-serif;\n  font-size: 112.5%;\n  margin: 0;\n  background-color: white;\n}\nheader {\n  position: fixed;\n  text-align: center;\n  background-color: slategray;\n  top: 0;\n  left: 0;\n  right: 0;\n  z-index: 100;\n}\n\nmain {\n  margin: 80px 40px 40px 40px;\n  overflow: scroll;\n  background-color: whitesmoke;\n  height: 100vh;\n  width: auto;\n}\n","src/scss/_utility.scss":".m-10 {\n  margin: 10px;\n}\n\n.radius-5 {\n  border-radius: 5px;\n}\n\n.display-none {\n  display: none;\n}\n","src/scss/_card.scss":".card {\n  position: relative;\n  border: 1px solid black;\n  min-height: 100px;\n  min-width: 200px;\n  border-radius: 5px;\n  box-shadow: 8px 8px 8px slategray;\n  padding: 15px;\n  margin-top: 30px;\n  margin-bottom: 40px;\n  margin-left: 10px;\n  margin-right: 10px;\n\n  &__question {\n    font-family: 'Quattrocento Sans', sans-serif;\n  }\n\n  &__bookmark {\n    position: absolute;\n    min-width: 60px;\n    min-height: auto;\n    top: -40px;\n    right: 20px;\n    height: 100px;\n    width: 20px;\n    cursor: pointer;\n    z-index: 50;\n  }\n\n  &__text {\n    font-family: 'Roboto', sans-serif;\n  }\n\n  &__tags {\n    display: flex;\n    flex-direction: start;\n    flex-wrap: wrap;\n    justify-content: sprace evenly;\n    gap: 20px;\n    padding: 0;\n  }\n\n  &__tag {\n    list-style: none;\n    display: flex;\n    font-size: 0 8em;\n    padding: 4px;\n    border: 1px solid black;\n    background-color: #ffb5b0;\n    border-radius: 5px;\n  }\n}\n","src/scss/_button.scss":".footer__navbar .current {\n  background-color: hsla(231, 0%, 67%, 0.33);\n}\n\nbutton {\n  display: block;\n  text-align: center;\n  background-color: grey;\n  padding: 5px;\n  margin: 5px;\n  text-align: center;\n  font-weight: bold;\n  font-size: 120%;\n  border-radius: 5%;\n}\n\n.card__bookmark--active path {\n  color: black;\n  fill: black !important;\n}\n\n.card__bookmark--inactive {\n  color: grey;\n  fill: grey;\n}\n\n.card__bookmark path {\n  color: grey;\n  fill: grey;\n}\n","src/scss/_footer.scss":".footer {\n  position: fixed;\n  height: 60px;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: slategray;\n  padding: 0 40px;\n  z-index: 100;\n\n  &__navbar {\n    display: flex;\n    height: 60px;\n    justify-content: space-around;\n    align-items: center;\n  }\n\n  &__link {\n    width: 60px;\n    height: auto;\n    text-align: center;\n  }\n  &__icon--home {\n    min-width: 40px;\n    min-height: auto;\n  }\n  &__icon--bookmarks {\n    min-width: 40px;\n    min-height: auto;\n  }\n\n  &__icon--cards {\n    min-width: 40px;\n    min-height: auto;\n  }\n  &__icon--profile {\n    min-width: 40px;\n    min-height: auto;\n  }\n}\n\n.footer__icon path {\n  color: rgb(44, 33, 45);\n  fill: rgb(44, 33, 45);\n}\n\n.footer__icon--active path {\n  color: black;\n  fill: black;\n}\n","src/scss/images.scss":".profile {\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n\n  &__image {\n    height: 150px;\n    width: auto;\n    border-radius: 50%;\n  }\n  &__name {\n    font-size: 80%;\n  }\n}\n\n.bio {\n  border: 1px solid transparent;\n  min-height: 100px;\n  min-width: 120px;\n  padding: 15px;\n  margin-bottom: 40px;\n  margin-top: 40px;\n\n  &__text {\n    font-style: italic;\n  }\n}\n","src/scss/_form.scss":"form {\n  display: flex;\n  flex-direction: column;\n}\n\n.input-question {\n  width: 100%;\n  padding: 20px;\n  text-indent: 20px;\n  margin: 10px;\n}\n\n.input-answer {\n  width: 100%;\n  padding: 20px;\n  margin: 10px;\n  text-indent: 20px;\n}\n\n.input-lables {\n  width: 100%;\n  padding: 10px;\n  margin: 10px;\n  text-indent: 20px;\n}\n\n.lable {\n  margin-top: 20px;\n}\n"},"lineCount":null}},"error":null,"hash":"5066233b261b07cfb57fbe4f8c9cb9d1","cacheData":{"env":{}}}