{
  "name": "quiz-appp",
  "version": "1.0.0",
  "description": "A simple app with questions and answers",
  "main": "index.html",
  "scripts": {
    "start": "npm run sass && npm-run-all --parallel serve sass:watch babel babel:watch",
    "babel": "babel src/js -d .",
    "babel:watch": "npm run babel -- -w",
    "serve": "serve .",
    "sass": "node-sass --output-style compressed src/scss/styles.scss styles.css",
    "sass:watch": "npm run sass -- -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/annaleaschroeder/quiz-appp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/annaleaschroeder/quiz-appp/issues"
  },
  "homepage": "https://github.com/annaleaschroeder/quiz-appp#readme",
  "dependencies": {
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4"
  }
}
